{
  "name": "Twitter Chrome Link Extension",
  "version": "1.0",
  "description": "Twitter Chrome Link Extension",
  "permissions": [
    "activeTab"
  ],
  "commands": { "showcontentdialog": { "suggested_key": { "default": "Ctrl+Shift+Y" }, "description": "show content dialog" } },
  "content_scripts": [{
    "matches": [
      "https://*.twitter.com/**"
    ],
    "css": ["content.css", "twitter.css"],
    "js": ["content.js", "twitter.js"],
    "run_at": "document_end"
  }],
  "manifest_version": 2
}
